{
  "name": "react-money-keyboard-simple",
  "version": "0.0.6",
  "description": "Lightweight and customizable React component that provides a virtual numeric keyboard designed specifically for money input fields.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chuihoc/react-money-keyboard-simple"
  },
  "author": "Chuihoc (https://github.com/chuihoc)",
  "bugs": {
    "url": "https://github.com/chuihoc/react-money-keyboard-simple/issues"
  },
  "homepage": "https://www.npmjs.com/package/react-money-keyboard-simple",
  "keywords": [
    "react",
    "reactjs",
    "digital",
    "keyboard",
    "onscreen",
    "virtual",
    "component",
    "screen-keyboard",
    "component",
    "virtual-keyboard",
    "touchscreen",
    "touch-screen",
    "kiosk",
    "osk",
    "js"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "tsup": "^8.0.1",
    "typescript": "^5.5.4"
  }
